<ul class="media-list">
  <% pictures.each do |picture| %>
    <% user = picture.user %>
    <li class="media">
      
      <div class="media-body">
        <div>
          <%= link_to user.name, user_path(user) %> <span class="text-muted">posted at <%= picture.created_at %></span>
        </div>
        <h2 id="mygraph"></h2>
        <div class="pic">
          <img class="media-object img-rounded" src="<%=picture.image.url %>" alt="">
        </div>
        <div>
          <% if current_user == picture.user %>
            <%= link_to "Delete", picture, method: :delete, data: { confirm: "You sure?" }, class: 'btn btn-danger btn-xs' %>
          <% end %>
        </div>
      </div>
    </li>
  <% end %>
  <%= paginate pictures %>
</ul>